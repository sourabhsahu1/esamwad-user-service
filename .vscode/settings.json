{
  "cSpell.words": [
    "Bhola",
    "ESAMWAD",
    "Gupshup",
    "Meity",
    "SMSC",
    "Umang",
    "artiomtr",
    "e",
    "fusionauth",
    "nestjs",
    "prebuild",
    "prismix",
    "samwad",
